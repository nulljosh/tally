{
  "name": "tally",
  "version": "1.6.1",
  "description": "Tally - BC Self-Serve Portal Scraper - Automated scraping of notifications, messages, payment info, and service requests",
  "main": "src/scraper.js",
  "scripts": {
    "check": "node src/scraper.js",
    "test": "node tools/test.js",
    "test:cors": "node tools/test-cors-utils.js",
    "test:auth-cookie": "node tools/test-auth-cookie.js",
    "test:productivity": "node tools/test-productivity.js",
    "test:scraper": "node src/scraper.js",
    "api": "node src/api.js",
    "dev": "nodemon src/api.js",
    "watch": "concurrently \"nodemon src/api.js\" \"browser-sync start --proxy localhost:3000 --files 'web/**/*.html' --no-open\"",
    "start": "node src/api.js",
    "upload-blob": "node scripts/upload-to-blob.js",
    "info": "bash scripts/tally-info.sh",
    "brute": "nodemon brute/tools/brute-force-web.js",
    "mock-server": "nodemon brute/mock-targets/mock-server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@sparticuz/chromium": "^143.0.4",
    "@vercel/blob": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "pac-resolver": "^7.0.1",
    "puppeteer-core": "^24.37.5"
  },
  "devDependencies": {
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.1",
    "live-server": "^1.2.2",
    "nodemon": "^3.1.11"
  }
}
